<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/first_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/first"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/first" />

    <TextView
        android:id="@+id/first"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/first_image"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/second_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/second"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/second" />

    <TextView
        android:id="@+id/second"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/second_image"
        app:layout_constraintTop_toBottomOf="@id/first" />

    <ImageView
        android:id="@+id/third_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/third"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/third" />

    <TextView
        android:id="@+id/third"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/third_image"
        app:layout_constraintTop_toBottomOf="@id/second" />

    <ImageView
        android:id="@+id/fourth_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/fourth"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/fourth" />

    <TextView
        android:id="@+id/fourth"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/fourth_image"
        app:layout_constraintTop_toBottomOf="@id/third" />

    <ImageView
        android:id="@+id/fifth_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/fifth"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/fifth" />

    <TextView
        android:id="@+id/fifth"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/fifth_image"
        app:layout_constraintTop_toBottomOf="@id/fourth" />

    <ImageView
        android:id="@+id/sixth_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/sixth"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/sixth" />

    <TextView
        android:id="@+id/sixth"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/sixth_image"
        app:layout_constraintTop_toBottomOf="@id/fifth" />

    <ImageView
        android:id="@+id/seventh_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/seventh"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/seventh" />

    <TextView
        android:id="@+id/seventh"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/seventh_image"
        app:layout_constraintTop_toBottomOf="@id/sixth" />

    <ImageView
        android:id="@+id/eighth_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/eighth"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/eighth" />

    <TextView
        android:id="@+id/eighth"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/eighth_image"
        app:layout_constraintTop_toBottomOf="@id/seventh" />

    <ImageView
        android:id="@+id/ninth_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/ninth"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/ninth" />

    <TextView
        android:id="@+id/ninth"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/ninth_image"
        app:layout_constraintTop_toBottomOf="@id/eighth" />

    <ImageView
        android:id="@+id/tenth_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/offset8"
        android:src="@drawable/ic_android_black_24dp"
        app:layout_constraintBottom_toBottomOf="@id/tenth"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/tenth" />

    <TextView
        android:id="@+id/tenth"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/offset8"
        android:text="@string/test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tenth_image"
        app:layout_constraintTop_toBottomOf="@id/ninth" />

</androidx.constraintlayout.widget.ConstraintLayout>